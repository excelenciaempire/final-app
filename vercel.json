{
  "version": 2,
  "builds": [
    {
      "src": "app-spediak/Spediak/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "app-spediak/Spediak/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/health.html"
    },
    {
      "src": "/(.*)",
      "dest": "/app-spediak/Spediak/dist/$1"
    }
  ],
  "env": {
    "EXPO_PUBLIC_API_URL": "@backend_url",
    "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "@clerk_publishable_key"
  },
  "github": {
    "silent": false,
    "autoJobCancelation": true
  }
}

